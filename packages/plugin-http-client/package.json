{
  "name": "@merkur/plugin-http-client",
  "version": "0.29.3",
  "description": "Merkur event emitter plugin.",
  "main": "lib/index",
  "module": "lib/index",
  "unpkg": "lib/index.umd.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./lib/index.es9.mjs": "./lib/index.es9.mjs",
    "./lib/index.es9.cjs": "./lib/index.es9.cjs"
  },
  "browser": {
    "./lib/index.mjs": "./lib/index.mjs",
    "./lib/index.es9.mjs": "./lib/index.es9.mjs"
  },
  "scripts": {
    "preversion": "npm test",
    "test": "../../node_modules/.bin/jest --no-watchman -c ./jest.config.js",
    "test:es:version": "../../node_modules/.bin/es-check es11 ./lib/index.mjs --module && ../../node_modules/.bin/es-check es9 ./lib/index.es9.mjs --module && ../../node_modules/.bin/es-check es9 ./lib/index.es9.cjs --module",
    "build": "node_modules/.bin/rollup -c",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjancarik/merkur.git"
  },
  "keywords": [
    "merkur",
    "plugin",
    "microservices",
    "microfrontends"
  ],
  "author": "Miroslav Jancarik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mjancarik/merkur/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://merkur.js.org/",
  "devDependencies": {
    "@merkur/core": "^0.29.3",
    "rollup": "^2.70.2"
  },
  "peerDependencies": {
    "@merkur/core": "*"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.6.7",
    "whatwg-fetch": "^3.6.2"
  }
}
