{
  "name": "@merkur/plugin-router",
  "version": "0.29.1",
  "description": "Merkur router plugin.",
  "main": "lib/index",
  "module": "lib/index",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./RouterEvents": {
      "import": "./lib/RouterEvents.mjs",
      "require": "./lib/RouterEvents.cjs"
    },
    "./lib/index.es9.mjs": "./lib/index.es9.mjs",
    "./lib/index.es9.cjs": "./lib/index.es9.cjs",
    "./lib/RouterEvents.es9.mjs": "./lib/RouterEvents.es9.mjs",
    "./lib/RouterEvents.es9.cjs": "./lib/RouterEvents.es9.cjs"
  },
  "browser": {
    "./lib/index.js": "./lib/index.es5.js",
    "./lib/index.cjs": "./lib/index.es5.js",
    "./lib/index.mjs": "./lib/index.mjs",
    "./lib/index.es9.mjs": "./lib/index.es9.mjs",
    "./lib/RouterEvents.js": "./lib/RouterEvents.es5.js",
    "./lib/RouterEvents.cjs": "./lib/RouterEvents.es5.js",
    "./lib/RouterEvents.mjs": "./lib/RouterEvents.mjs",
    "./lib/RouterEvents.es9.mjs": "./lib/RouterEvents.es9.mjs"
  },
  "scripts": {
    "preversion": "npm test",
    "test": "../../node_modules/.bin/jest --no-watchman -c ./jest.config.js",
    "test:es:version": "../../node_modules/.bin/es-check es5 ./lib/index.es5.js && ../../node_modules/.bin/es-check es11 ./lib/index.mjs --module && ../../node_modules/.bin/es-check es9 ./lib/index.es9.mjs --module && ../../node_modules/.bin/es-check es9 ./lib/index.es9.cjs --module",
    "build": "node_modules/.bin/rollup -c",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjancarik/merkur.git"
  },
  "keywords": [
    "merkur",
    "plugin",
    "router"
  ],
  "author": "Miroslav Jancarik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mjancarik/merkur/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://merkur.js.org/",
  "devDependencies": {
    "@merkur/core": "^0.29.1",
    "@merkur/plugin-component": "^0.29.1",
    "@merkur/plugin-event-emitter": "^0.29.1",
    "rollup": "^2.70.2"
  },
  "peerDependencies": {
    "@merkur/core": "*",
    "@merkur/plugin-component": "*",
    "@merkur/plugin-event-emitter": "*"
  },
  "dependencies": {
    "universal-router": "^9.1.0"
  }
}
