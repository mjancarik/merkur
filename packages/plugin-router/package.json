{
  "name": "@merkur/plugin-router",
  "version": "0.32.0",
  "description": "Merkur router plugin.",
  "main": "lib/index",
  "module": "lib/index",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./RouterEvents": {
      "import": "./lib/RouterEvents.mjs",
      "require": "./lib/RouterEvents.cjs"
    },
    "./lib/index.es9.mjs": "./lib/index.es9.mjs",
    "./lib/index.es9.cjs": "./lib/index.es9.cjs",
    "./lib/RouterEvents.es9.mjs": "./lib/RouterEvents.es9.mjs",
    "./lib/RouterEvents.es9.cjs": "./lib/RouterEvents.es9.cjs"
  },
  "browser": {
    "./lib/index.js": "./lib/index.js",
    "./lib/index.cjs": "./lib/index.cjs",
    "./lib/index.mjs": "./lib/index.mjs",
    "./lib/index.es9.mjs": "./lib/index.es9.mjs",
    "./lib/RouterEvents.js": "./lib/RouterEvents.js",
    "./lib/RouterEvents.cjs": "./lib/RouterEvents.cjs",
    "./lib/RouterEvents.mjs": "./lib/RouterEvents.mjs",
    "./lib/RouterEvents.es9.mjs": "./lib/RouterEvents.es9.mjs"
  },
  "scripts": {
    "preversion": "npm test",
    "test": "jest --no-watchman -c ./jest.config.js",
    "test:es:version": "es-check es11 ./lib/index.mjs --module && es-check es9 ./lib/index.es9.mjs --module && es-check es9 ./lib/index.es9.cjs --module",
    "build": "rollup -c rollup.config.mjs",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjancarik/merkur.git"
  },
  "keywords": [
    "merkur",
    "plugin",
    "router"
  ],
  "author": "Miroslav Jancarik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mjancarik/merkur/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://merkur.js.org/",
  "devDependencies": {
    "@merkur/core": "^0.32.0",
    "@merkur/plugin-component": "^0.32.0",
    "@merkur/plugin-event-emitter": "^0.32.0"
  },
  "peerDependencies": {
    "@merkur/core": "*",
    "@merkur/plugin-component": "*",
    "@merkur/plugin-event-emitter": "*"
  },
  "dependencies": {
    "universal-router": "^9.1.0"
  }
}
