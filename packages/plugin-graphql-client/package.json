{
  "name": "@merkur/plugin-graphql-client",
  "version": "0.44.0",
  "description": "Merkur event emitter plugin.",
  "main": "lib/index",
  "module": "lib/index",
  "unpkg": "lib/index.umd.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./lib/index.es9.mjs": "./lib/index.es9.mjs",
    "./lib/index.es9.cjs": "./lib/index.es9.cjs",
    "./webpack": "./webpack.js",
    "./webpack/": "./webpack.js",
    "./webpack.js": "./webpack.js"
  },
  "browser": {
    "./lib/index.js": "./lib/index.js",
    "./lib/index.cjs": "./lib/index.cjs",
    "./lib/index.mjs": "./lib/index.mjs",
    "./lib/index.es9.mjs": "./lib/index.es9.mjs"
  },
  "scripts": {
    "preversion": "npm test",
    "test": "jest --no-watchman -c ./jest.config.js",
    "test:es:version": "es-check es11 ./lib/index.mjs --module && es-check es9 ./lib/index.es9.mjs --module && es-check es9 ./lib/index.es9.cjs --module",
    "build": "rollup -c rollup.config.mjs",
    "postpublish": "node ../../utils/restoreLatestTag.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjancarik/merkur.git"
  },
  "keywords": [
    "merkur",
    "plugin",
    "microservices",
    "microfrontends"
  ],
  "author": "Miroslav Jancarik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mjancarik/merkur/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://merkur.js.org/",
  "devDependencies": {
    "@merkur/core": "^0.44.0",
    "@merkur/plugin-component": "^0.44.0",
    "@merkur/plugin-error": "^0.44.0",
    "@merkur/plugin-http-client": "^0.44.0"
  },
  "peerDependencies": {
    "@merkur/core": "*",
    "@merkur/plugin-error": "*",
    "@merkur/plugin-http-client": "*"
  },
  "dependencies": {
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6"
  }
}
